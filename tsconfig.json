  "include": ["./src"],
  "exclude": ["./src/exclude"],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "lib": ["es6", "es2017", "dom"],
    "moduleResolution": "node",
    "outDir": "./lib",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6"
  }
}